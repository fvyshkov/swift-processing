    "methods": {},
    "references": {
        "default": {
            "form": {
                "style": {
                    "width": "1000px",
                    "height": "600px"
                },
                "columns": [
                    {
                        "title": "Код",
                        "field": "code",
                        "width": 100
                    },
                    {
                        "title": "Наименование",
                        "field": "name",
                        "flex": 1
                    }
                ],
                "fastFilter": false,
                "primaryKey": "code",
                "isGroup": "item.groupFl=='1'",
                "isSelectable": "node.data.groupFl!='1'"
            },
            "method": {
                "sql": {
                    "params": [
                        "productCode",
                        "code"
                    ],
                    "sql": "select l.dcl_id,\r\n       to_char(l.lim) \"code\",\r\n       substr(bs_dom.dlongname('LLINE_LIM_TYPE',l.lim),1,250) \"name\"\r\nfrom l_line_lim l, t_deacls_std p\r\nwhere l.dcl_id=p.id\r\n    and (:productCode is null or p.code = :productCode)\r\n    and nvl(:code, l.lim) = l.lim\r\norder by code",
                    "sqlType": "query",
                    "database": "bank"
                },
                "script": {
                    "params": [],
                    "py": ""
                }
            }
        }
    },
    "lists": {},
    "js": {}
}