{
    "lists": {},
    "forms": {
        "listFilterFrame": {
            "title": "Filter",
            "style": {
                "width": "360px",
                "overflow": "hidden",
                "paddingRight": "0px"
            },
            "className": "panel vertical",
            "$": {
                "@fields": {
                    "className": "vertical",
                    "style": {
                        "paddingTop": "8px",
                        "paddingRight": "8px",
                        "overflowY": "auto",
                        "flexGrow": 1
                    },
                    "$$": "params.attributes || {}"
                },
                "@buttons": {
                    "className": "horizontal",
                    "$": {
                        "btnClear": {
                            "control": "Button",
                            "label": "Clear",
                            "controlProps": {
                                "variant": "outlined",
                                "color": "primary"
                            },
                            "action": {
                                "name": "clean"
                            }
                        },
                        "btnApply": {
                            "control": "Button",
                            "label": "Apply",
                            "controlProps": {
                                "variant": "contained",
                                "color": "primary"
                            },
                            "action": {
                                "js": "actions.apply(mem);",
                                "params": {
                                    "disableUpdate": true
                                }
                            }
                        }
                    }
                }
            },
            "actions": {
                "clean": {
                    "jsScript": "Object.keys(mem).forEach(function(key) { delete mem[key]; });"
                }
            }
        },
        "saveCancelDialogButtons": {
            "className": "horizontal",
            "style": {
                "flexDirection": "row-reverse"
            },
            "$": {
                ".btnAppy": {
                    "label": "Save",
                    "control": "Button",
                    "controlProps": {
                        "color": "primary",
                        "variant": "contained",
                        "style": {
                            "textTransform": "none"
                        }
                    },
                    "action": [
                        {
                            "js": "if (!validate()) throw 'Abort';"
                        },
                        {
                            "js": "action({name: params.onSave})"
                        },
                        {
                            "js": "actions.close();",
                            "disableUpdate": true
                        }
                    ]
                },
                ".btnCancel": {
                    "label": "Cancel",
                    "control": "Button",
                    "controlProps": {
                        "color": "primary",
                        "style": {
                            "textTransform": "none"
                        }
                    },
                    "action": {
                        "js": "actions.close();",
                        "disableUpdate": true
                    }
                }
            }
        },
        "systemIconsTask": {
            "className": "task-panel",
            "$": {
                "|systemIcons": {
                    "style": {
                        "height": "100%"
                    },
                    "control": "ListTable",
                    "controlProps": {
                        "style": {
                            "height": "100%"
                        },
                        "gridOptions": {
                            "domLayout": "normal"
                        },
                        "columns!": {
                            "icon": {
                                "icon$": "console.log('icon', params) || params.data?.name"
                            },
                            "name": {
                                "flex": 1
                            }
                        }
                    }
                }
            },
            "events": {
                "onTaskCreated": {
                    "js": "console.log(app); mem.systemIcons = [...[...app.ui.namedIcons.keys()].sort().map((icon)=>({name: icon}))];"
                }
            }
        }
    },
    "js": {},
    "methods": {},
    "references": {}
}